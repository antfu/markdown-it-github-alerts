{
  "name": "markdown-it-github-alerts",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "pnpm@10.5.2",
  "description": "Support GitHub-style alerts for markdown-it",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/markdown-it-github-alerts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/markdown-it-github-alerts.git"
  },
  "bugs": "https://github.com/antfu/markdown-it-github-alerts/issues",
  "keywords": [
    "markdown-it",
    "github-favored-markdown"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./styles/*": "./styles/*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "markdown-it": ">= 13.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@antfu/ni": "^23.3.1",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.13.5",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "lint-staged": "^15.4.3",
    "markdown-it": "^14.1.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.8.0",
    "typescript": "^5.7.3",
    "unbuild": "^3.5.0",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
